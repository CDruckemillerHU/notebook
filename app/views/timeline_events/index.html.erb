<p id="notice"><%= notice %></p>

<h1>Timeline Events</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @timeline_events.each do |timeline_event| %>
      <tr>
        <td><%= link_to 'Show', timeline_event %></td>
        <td><%= link_to 'Edit', edit_timeline_event_path(timeline_event) %></td>
        <td><%= link_to 'Destroy', timeline_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Timeline Event', new_timeline_event_path %>
